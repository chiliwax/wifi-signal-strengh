{
  "cli": {
    "version": ">= 3.9.3"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "android": {
        "autoIncrement": "versionCode",
        "distribution": "production"
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "track": "production"
      }
    },
    "internal": {
      "android": {
        "track": "internal"
      }
    }
  }
}
